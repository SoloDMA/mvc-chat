@model IEnumerable<RoomModel>



<div id="place-for-rooms">
    @if (Model != null)
    {
        @foreach (var room in Model)
        {
            <a asp-controller="Home" asp-action="Index" asp-route-currentRoomId="@room.Id" class="room">
                <img src="https://img.icons8.com/flat_round/64/000000/filled-chat.png" />
                <span class="room-name">@room.Name</span>
            </a>
        }
    }
</div>
